p.trans <- function(p) {
res <- as.numeric(NA, length(p))
for (i in 1:length(p)) {
if (p[i] > 0.1) res[i] <- "p > 0.05 [NS]"
if (p[i] > 0.05 & p[i] <= 0.1) res[i] <- "p < 0.1 [.]"
if (p[i] > 0.01 & p[i] <= 0.05) res[i] <- "p < 0.05 [*]"
if (p[i] > 0.001 & p[i] <= 0.01) res[i] <- "p < 0.01 [**]"
if (p[i] <= 0.001) res[i] <- "p < 0.001 [***]"
}
return(res)}
